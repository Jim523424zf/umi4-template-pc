{
  "name": "umi4-template",
  "private": true,
  "author": "Alioth Zhou <jun.zhou@medalsoft.com>",
  "scripts": {
    "dev": "cross-env PORT=8020 UMI_ENV=dev max dev",
    "build": "pnpm build:locales && max build",
    "build:prod": "pnpm build:locales && cross-env UMI_ENV=prod max build",
    "build:test": "pnpm build:locales && cross-env UMI_ENV=test max build",
    "build:uat": "pnpm build:locales && cross-env UMI_ENV=uat max build",
    "format": "prettier --cache --write src",
    "lint": "npx --no-install lint-staged --quiet",
    "prepare": "husky install",
    "postinstall": "max setup",
    "setup": "max setup",
    "start": "pnpm i && pnpm dev",
    "openapi": "max openapi && pnpm format",
    "build:locales": "node ./scripts/locales-2-json.cjs",
    "locales": "node ./scripts/locales.cjs && pnpm format",
    "locales:export": "node ./scripts/locales-export.cjs"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "@ant-design/pro-components": "^2.6.28",
    "@azure/msal-browser": "^3.4.0",
    "@medalsoft/umi-plugins": "0.0.10-4",
    "@microsoft/teams-js": "^2.18.0",
    "@umijs/max": "^4.0.83",
    "ahooks": "^3.7.8",
    "antd": "^5.13.0",
    "antd-style": "^3.5.0",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.9",
    "inversify": "^6.0.2",
    "jsonwebtoken": "^9.0.2",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "loglevel": "^1.8.1",
    "query-string": "^8.1.0",
    "react-intl": "3.12.1",
    "reflect-metadata": "^0.2.1",
    "streamsaver": "^2.0.6"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.4",
    "@types/lodash-es": "^4.17.8",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/streamsaver": "^2.0.1",
    "basic-ftp": "^5.0.3",
    "cross-env": "^7.0.3",
    "exceljs": "^4.3.0",
    "fontmin": "^1.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "opentype.js": "^1.3.4",
    "postcss-px-to-viewport-8-plugin": "^1.2.2",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-packagejson": "^2.4.3",
    "swagger-ui-dist": "^5.1.0",
    "typescript": "^5.1.6"
  }
}